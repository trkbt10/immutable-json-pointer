{
  "name": "immutable-json-pointer",
  "version": "1.3.0",
  "description": "JSON Pointer implementation for Node.js",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./typed": {
      "types": "./dist/typed-json-pointer.d.ts",
      "import": "./dist/typed-json-pointer.js",
      "require": "./dist/typed-json-pointer.cjs",
      "default": "./dist/typed-json-pointer.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "benchmark": "ts-node benchmark/index.ts"
  },
  "keywords": [
    "jsonpointer",
    "json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/trkbt10/immutable-json-pointer.git"
  },
  "bugs": {
    "url": "https://github.com/trkbt10/immutable-json-pointer/issues"
  },
  "author": "trkbt10 <triplequartet@gmail.com>",
  "license": "Unlicense",
  "devDependencies": {
    "@types/node": "^24.0.0",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.6"
  }
}
